{
  "name": "@synnaxlabs/client",
  "private": false,
  "version": "0.3.0",
  "type": "module",
  "description": "The Client Library for Synnax",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/client/ts",
  "keywords": [
    "synnax",
    "grpc",
    "transport",
    "client",
    "data acquisition",
    "big data",
    "telemetry",
    "control systems"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "watch": "tsc --noEmit && vite build --watch",
    "test": "vitest",
    "cov": "vitest --coverage",
    "lint": "eslint \"src/**/*.ts*\"",
    "fix": "eslint \"src/**/*.ts*\" --fix"
  },
  "dependencies": {
    "@synnaxlabs/freighter": "workspace:*",
    "@synnaxlabs/x": "workspace:*",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@synnaxlabs/tsconfig": "workspace:*",
    "@synnaxlabs/vite-plugin": "workspace:*",
    "@types/node": "^20.3.0",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint-config-synnaxlabs": "workspace:*",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  },
  "main": "dist/client.cjs.js",
  "module": "dist/client.es.js",
  "types": "dist/index.d.ts"
}
